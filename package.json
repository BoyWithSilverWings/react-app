{
  "name": "react-app",
  "version": "1.0.0",
  "description": "A React Boilerplate Configuration",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli --mode=production --env=prod",
    "start": "webpack-dev-server --mode=development --env=dev",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "Boy With Silver Wings <agney@outlook.in>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.4.4",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.12",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.7",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "jest-dom": "^3.5.0",
    "lint-staged": "^4.3.0",
    "mini-css-extract-plugin": "^0.4.5",
    "npm-install-webpack-plugin": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.4",
    "webpack-monitor": "^1.0.14",
    "workbox-webpack-plugin": "^2.1.3"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/scripts/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules",
      "/scripts",
      "/build-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BoyWithSilverWings/react-app.git"
  }
}
